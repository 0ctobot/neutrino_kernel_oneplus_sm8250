};

&thermal_zones {
	skin_therm_19821: skin-therm {
		trips {
			skint_therm_trip0: skint_therm_trip0 {
				temperature = <46000>;
				hysteresis = <2000>;
				type = "passive";
			};
			skint_therm_trip1: skint_therm_trip1 {
				temperature = <47000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skint_therm_trip2: skint_therm_trip2 {
				temperature = <48000>;
				hysteresis = <2000>;
				type = "passive";
			};
			skint_therm_trip3: skint_therm_trip3 {
				temperature = <50000>;
				hysteresis = <2000>;
				type = "passive";
			};
		};
		cooling-maps {
			freq_dev0 {
				trip = <&skint_therm_trip0>;
