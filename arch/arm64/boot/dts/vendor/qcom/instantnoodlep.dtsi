/* @bsp, 2019/10/08 usb config START*/
&usb2_phy0 {
	qcom,param-override-seq =
		<0x67 0x6c/*Disconnection voltage +21.56%*/
		0x0b 0x70/*Pre-emphasis:4x DC voltage level:+17.80%*/
		0x28 0x74>;
};

&usb1 {
	status = "disabled";
};

&thermal_zones {
	skin-therm {
		trips {
			freq_config0: freq_config0 {
				temperature = <46000>;
				hysteresis = <2000>;
				type = "passive";
			};
			freq_config1: freq_config1 {
				temperature = <47000>;
				hysteresis = <1000>;
				type = "passive";
			};
			freq_config2: freq_config2 {
				temperature = <49000>;
				hysteresis = <2000>;
				type = "passive";
			};
			freq_config3: freq_config3 {
				temperature = <50000>;
				hysteresis = <2000>;
				type = "passive";
